datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Discount {
  createdAt      DateTime  @default(now())
  discountType   String?
  expirationDate DateTime?
  id             String    @id @default(cuid())
  updatedAt      DateTime  @updatedAt
  user           String?
}

model Category {
  createdAt DateTime @default(now())
  icon      Json?
  id        String   @id @default(cuid())
  title     String?
  updatedAt DateTime @updatedAt
}

model Profile {
  createdAt DateTime @default(now())
  id        String   @id @default(cuid())
  updatedAt DateTime @updatedAt
}

model PushNotification {
  createdAt DateTime @default(now())
  id        String   @id @default(cuid())
  updatedAt DateTime @updatedAt
}

model News {
  createdAt DateTime @default(now())
  id        String   @id @default(cuid())
  updatedAt DateTime @updatedAt
}
